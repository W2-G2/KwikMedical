@model KwikMedical.Models.Ambulance

<h2>Are you sure you want to delete this ambulance?</h2>

<p>
    <strong>Code:</strong> @Model.Code <br>
    <strong>Current City:</strong> @Model.CurrentCity
</p>

<form asp-action="DeleteConfirmed" asp-route-id="@Model.Id">
    <input type="submit" value="Delete" class="btn btn-danger" />
    <a asp-action="Index" class="btn btn-secondary">Cancel</a>
</form>
